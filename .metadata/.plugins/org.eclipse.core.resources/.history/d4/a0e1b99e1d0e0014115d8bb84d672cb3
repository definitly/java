
public class CargoPlane extends Aircraft {
	
	public double maxpayload; // максимальная загрузка
	public double payload;    // загрузка самолета
	public double percentload; //загрузка самолета в процентах
	
	public double percent() {
		
		percentload=(payload/maxpayload)*100;
		
		return percentload;
	
	}
	

}
